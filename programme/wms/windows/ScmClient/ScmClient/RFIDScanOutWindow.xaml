<Window x:Class="ScmClient.RFIDScanOutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScmClient"
        mc:Ignorable="d"
        Title="RFIDScanOutWindow" Height="500" Width="800" Loaded="Window_Loaded"
        WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid Grid.Row="2">
        <Grid.RowDefinitions>
            <RowDefinition Height="90*"/>
            <RowDefinition Height="10*"/>
        </Grid.RowDefinitions>
        <Frame x:Name="NaviFrame" Grid.Row="0" NavigationUIVisibility="Hidden" Cursor="Hand"/>
        <Button x:Name="BackBtn" Content="放弃" HorizontalAlignment="Left" Margin="10,2,0,0" Grid.Row="1" VerticalAlignment="Top" Width="68" Height="32" Click="BackBtn_Click"/>
        <Button x:Name="NextBtn" Content="下一步" HorizontalAlignment="Left" Margin="509,2,0,0" Grid.Row="1" VerticalAlignment="Top" Width="78" Click="NextBtn_Click" Height="32" />
    </Grid>
</Window>
