<Window x:Class="ScmClient.RFIDScanInWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScmClient"
        mc:Ignorable="d"
        Title="RFIDScanInWindow" Height="312" Width="900" Loaded="Window_Loaded"
        WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid Grid.Row="2">
        <Grid.RowDefinitions>
            <RowDefinition Height="90*"/>
            <RowDefinition Height="10*"/>
        </Grid.RowDefinitions>
        <Frame x:Name="NaviFrame" Grid.Row="0" NavigationUIVisibility="Visible" Cursor="None"/>
        <Button x:Name="BackBtn" Content="返回" HorizontalAlignment="Left" Margin="10,2,0,0" Grid.Row="1" VerticalAlignment="Top" Width="46" Height="23" Click="BackBtn_Click"/>
    </Grid>
</Window>
