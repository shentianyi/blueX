<p id="notice"><%= notice %></p>

<h1>Listing Storages</h1>

<table>
  <thead>
    <tr>
      <th>Part</th>
      <th>Fifo</th>
      <th>Quantity</th>
      <th>Package nr</th>
      <th>Uniq</th>
      <th>Position</th>
      <th>Warehouse</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @storages.each do |storage| %>
      <tr>
        <td><%= storage.part_id %></td>
        <td><%= storage.fifo %></td>
        <td><%= storage.quantity %></td>
        <td><%= storage.package_nr %></td>
        <td><%= storage.uniq_nr %></td>
        <td><%= storage.position_id %></td>
        <td><%= storage.warehouse_id %></td>
        <td><%= link_to 'Show', storage %></td>
        <td><%= link_to 'Edit', edit_storage_path(storage) %></td>
        <td><%= link_to 'Destroy', storage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Storage', new_storage_path %>
