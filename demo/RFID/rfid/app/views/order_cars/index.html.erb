<p id="notice"><%= notice %></p>

<h1>Listing Order Cars</h1>

<table>
  <thead>
    <tr>
      <th>Nr</th>
      <th>Rfid nr</th>
      <th>Warehouse</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_cars.each do |order_car| %>
      <tr>
        <td><%= order_car.nr %></td>
        <td><%= order_car.rfid_nr %></td>
        <td><%= order_car.warehouse_id %></td>
        <td><%= order_car.status %></td>
        <td><%= link_to 'Show', order_car %></td>
        <td><%= link_to 'Edit', edit_order_car_path(order_car) %></td>
        <td><%= link_to 'Destroy', order_car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order car', new_order_car_path %>
